#include<stdio.h>
int main()
{
    int n;
    printf("enter the number of elements:");
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int m;
    scanf("%d",&m);
    for(int i=0;i<n;i++)
    {
        if(a[i]>m)
        {
            printf("%d",i);
            break;
        }
    }
}
